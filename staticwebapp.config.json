{
  "routes": [
    {
      "route": "/:year/:month/:day/:slug",
      "redirect": "https://stephenoravec.wordpress.com/:year/:month/:day/:slug",
      "statusCode": 301
    },
    {
      "route": "/:year/:month/:day/:slug/",
      "redirect": "https://stephenoravec.wordpress.com/:year/:month/:day/:slug",
      "statusCode": 301
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}